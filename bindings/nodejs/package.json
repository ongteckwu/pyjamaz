{
  "name": "pyjamaz",
  "version": "1.0.0",
  "description": "Node.js bindings for Pyjamaz - high-performance image optimizer",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:ts": "jest --testMatch='**/*.test.ts'",
    "test:js": "jest --testMatch='**/*.test.js'",
    "test:coverage": "jest --coverage",
    "lint": "eslint src tests examples --ext .ts,.js",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.{ts,js}\" \"examples/**/*.{ts,js}\"",
    "prepublishOnly": "npm run build",
    "postinstall": "node install.js"
  },
  "files": [
    "dist/",
    "native/",
    "install.js",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "image",
    "optimization",
    "compression",
    "perceptual",
    "jpeg",
    "png",
    "webp",
    "avif",
    "dssim",
    "ssimulacra2"
  ],
  "author": "Pyjamaz Team",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "ffi-napi": "^4.0.3",
    "ref-napi": "^3.0.3",
    "ref-struct-di": "^1.1.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/pyjamaz.git",
    "directory": "bindings/nodejs"
  },
  "bugs": {
    "url": "https://github.com/yourusername/pyjamaz/issues"
  },
  "homepage": "https://github.com/yourusername/pyjamaz#readme"
}
